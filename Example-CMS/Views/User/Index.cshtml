@model PagedList.IPagedList<Example_CMS.ViewModel.UserViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model[0].IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].DtCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].DtModified)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].FullControl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].Password)
        </th>
        <th></th>
    </tr>
    <span class="list-user">
        @Html.Partial("_ListUsers", Model)
    </span>
</table>